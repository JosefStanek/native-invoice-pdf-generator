{
  "expo": {
    "name": "invoice generator",
    "slug": "invoice-generator",
    "description": "Invoice generator for android.",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "userInterfaceStyle": "automatic",
      "supportsTablet": true
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.josefstanek.invoicegenerator"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "51.0.0",
    "extra": {
      "eas": {
        "projectId": "4205cf22-714c-483f-9b75-24c58dc2efec"
      }
    }
  }
}
